@using CxAPI_Store
<!DOCTYPE html>
<html>
<head>
<style>
td, th {
  border: 1px solid black;
  word-wrap:break-word
}
table {width: 100%;
    border: 1px solid black;
	border-collapse: collapse;
	table-layout: fixed;
	font-family: helvetica;
	}	
h1   {color: blue;
	 font-family: helvetica;
	 }
h2   {color: blue;
	 font-family: helvetica;
	 }
h3   {color: blue;
	font-family: helvetica;
	 }
.small {color: blue;
	font-family: helvetica;
	font-size: x-small;
	 }
.debug {color: red;
	font-family: helvetica;
	font-size: x-small;
	 }
p,thead,td,th{font-family: helvetica;
	font-size: small;
	 }
.High { color: red;
		font-weight: bold;}
.Medium { color: deeppink;
       font-weight: bold; }
.Low { color: green; 
       font-weight: bold; }
.Info { color: blue; 
       font-weight: bold; }
img {
	width: 25%;
	height: auto; }
.pagebreak {
		page-break-before: always;}
</style>
</head>
<body>
<img src="data:image/png;base64,"/>
<h3>Checkmarx Static Source Code Testing Report</h3>

Prepared on: @Model.runDate </br>
Business Application: @Model.businessName

<h3>Report Contents</h3>
<ul style="list-style-type:none;">
<li><a href="#summary">Executive summary</a></li>	
<li><a href="#report1">Risk over previous 6 months</a></li>	
<li><a href="#report2">Finding by Severity (trend in comparison to last month’s scan)</a></li>	
<li><a href="#report3">Finding by Category (trend in comparison to last month’s scan)</a></li>	
<li><a href="#report4">Open findings from previous scans</a></li>	
<li><a href="#report5">Scope of the Scan</a></li> 	
<li><a href="#report6">Detailed Findings by Severity ( Result State – that are still open/ confirmed as exploitable and need fixing)</a></li>	
</ul>

<ul style="list-style-type:none">
<li><a href="#appendix">Appendix A</a></li>
<ul style="list-style-type:none">
<li><a href="#appendix">Checkmarx Methodology</a></li>	
<ul style="list-style-type:none">
<li><a href="#appendix">About Checkmarx Risk Score calculation</a></li>
</ul>
</ul>
</ul>


<div class="pagebreak"></div>



<a name="summary"><h3>Executive summary</h3>

<p>This report contains a summary of the findings identified in the application using automated static code analysis techniques. The report provides a high level risk trend over the previous twelve month period.</p>

<h3>Overall Risk Score for: <span style="font-weight:bold;color:black">@Model.businessName</span></h3>

<h3 class="@Model.overAllSeverity">@Model.overAllSeverity</h3>
</a>


<a name="report1"><h3>Risk over previous 6 months </h3>


@{
    var myChart = new Chart(width: 600, height: 400)
        .AddTitle("Chart Title")
        .AddSeries(
            name: "Employee",
            xValue: new[] {  "Peter", "Andrew", "Julie", "Mary", "Dave" },
            yValues: new[] { "2", "6", "4", "5", "3" })
        .Write();
}

<table>
    <thead>
        <tr>
            <th>Projects</th>
		    @foreach (var month in Model.mbn.lastmonth) {
            <th>@month</th>
			}
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.dataSet.Tables["months"].Rows) {
			<tr>
				<td>@item["ComponentName"]</td>
				<td>@item["MonthRisk_0"]</td>
				<td>@item["MonthRisk_1"]</td>
				<td>@item["MonthRisk_2"]</td>
				<td>@item["MonthRisk_3"]</td>
				<td>@item["MonthRisk_4"]</td>
				<td>@item["MonthRisk_5"]</td>
				<td>@item["MonthRisk_6"]</td>				
				<td>@item["MonthRisk_7"]</td>				
			</tr>
        }
    </tbody>
</table>
</a>
<a name="report2"><h3>Findings by Severity (trend in comparison to last month's scan)</h3>
<table>
	<thead>
		<tr>
			<th>Severity</th>
			<th>SAST Scan - Previous Month</th>
			<th>SAST Scan - Current Month</th>
			<th>Trend (+/-)</th>
		</tr>	
	</thead>
	<tbody>
		@foreach (var item in Model.dataSet.Tables["severities"].Rows) {
			<tr>
				<td>@item["Severity"]</td>
				<td>@item["PreviousMonth"]</td>
				<td>@item["CurrentMonth"]</td>
				<td>@item["Difference"]</td>
			</tr>
		}
	</tbody>
</table>
<span class="small">Trend is positive as vulerabilities are reduced, and negative when it increases</span>
<!--<span class="debug"> Last scan High: @Model.allCounts["High"], Medium: @Model.allCounts["Medium"] , Low: @Model.allCounts["Low"], Info: @Model.allCounts["Info"] </span> -->

</a>
<a name="report3"><h3>Findings by Category (trend in comparison to last month's scan)</h3>
<table>
	<thead>
		<tr>
			<th>OWASP Category</th>
			<th>SAST Scan - Previous Month</th>
			<th>SAST Scan - Current Month</th>
			<th>Trend (+/-)</th>
		</tr>	
	</thead>
	<tbody>
		@foreach (var item in Model.dataSet.Tables["OWASPCategory"].Rows) {
			<tr>
				<td>@item["OWASPCategory"]</td>
				<td>@item["PreviousMonth"]</td>
				<td>@item["CurrentMonth"]</td>
				<td>@item["Difference"]</td>
			</tr>
		}
	</tbody>
</table>
<span class="small">Trend is positive as vulerabilities are reduced, and negative when it increases</span>
</a>
<a name="report4"><h3>Open Findings from previous scans</h3>
<table>
	<thead>
		<tr>
			<th>Projects</th>
			<th>Severity</th>
			<th>SourceFile</th>
			<th>DestinationFile</th>
			<th>QueryName</th>
			<th>DeepLink</th>
			<th>First Found</th>
			<th>Policy Violation</th>			
		</tr>	
	</thead>
	<tbody>
		@foreach (var item in Model.dataSet.Tables["openFindingsViolations"].Rows) {
			<tr>
				<td>@item["ComponentName"]</td>
				<td>@item["Severity"]</td>
				<td>@item["SourceFile"]</td>
				<td>@item["DestinationFile"]</td>
				<td>@item["Query"]</td>
				<td><a href="@item["DeepLink"]">@Model.businessName/@item["ComponentName"]</a></td>
				<td>@item["FirstFound"]</td>
				<td>&gt;&nbsp;@item["PolicyViolation"]</td>
			</tr>
		}
	</tbody>
</table>
<span class="small"> Policy settings, High: &gt; @Model.policy["High"] days, Medium: &gt; @Model.policy["Medium"] days, Low: &gt; @Model.policy["Low"] days, Info: &gt; @Model.policy["Info"] days</span>
</a>
<a name="report5"><h3>Scope of the scan</h3>
<table>
	<thead>
		<tr>
			<th>Projects</th>
			<th>Languages Scanned</th>
			<th>LOC</th>		
		</tr>	
	</thead>
	<tbody>
		@foreach (var item in Model.dataSet.Tables["scopeOfScan"].Rows) {
			<tr>
				<td>@item["ComponentName"]</td>
				<td>@item["Languages"]</td>
				<td>@item["LOC"]</td>
			</tr>
		}
	</tbody>
</table>
</a>
<a name="report6"><h3>Detailed Findings by Severity</h3>
<p>(Result State – that are still open/ confirmed as exploitable and need fixing)</p>


@foreach (var key in Model.messy.Keys) {
	<p>@key - @Model.nCounts[@key]</p>
	foreach (var table in Model.messy[key].Tables)
	{
	<table>
		<thead>
			<tr>
				<th>Project</th>
				<th>SourceFile</th>
				<th>DestinationFile</th>
				<th>QueryName</th>
				<th>DeepLink</th>
				<th>First Found</th>	
			</tr>	
		</thead>
		<tbody>
			@foreach (var item in table.Rows) {
				<tr>
					<td>@item["ComponentName"]</td>
					<td>@item["SourceFile"]</td>
					<td>@item["DestinationFile"]</td>
					<td>@item["Query"]</td>
					<td><a href="@item["DeepLink"]">@Model.businessName/@item["ComponentName"]</a></td>
					<td>@item["FirstFound"]</td>
				</tr>
			}
		</tbody>
	</table>
	<br/>
	}
<br/>
}
</a>
</div>
<div class="pagebreak">
<a name="appendix"><h2>Appendix A</h2>
<h3>Checkmarx Methodology</h3>

<h3>About Checkmarx Risk Score calculation</h3>
<p>Calculate High score:</p>
<p>if ( number of the High results > 20)
then  High score =Min ((the number of high score results -20)*1.5 , 100)
Else High score = 3</p>

<p>Calculate Medium score:</p>
<p>Medium score = minimum between (30) to (number of medium results * 1.5);</p>

<p>Calculate Low score:</p>
<p>Low score = minimum between (10) to (number of the low * 0.5)</p>

<!--<h3>Cx Finding Severity Definition</h3>

<p>Risk level: min (calculate High score + calculate Medium score + calculate Low score, 100)</p>-->
</a>
</div>


</body>
</html>